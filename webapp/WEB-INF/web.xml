<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>FirstServlet</servlet-name>
		<servlet-class>FirstServletProg</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
		<url-pattern>/first</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FirstHttpServletprog</servlet-name>
		<servlet-class>"FirstHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FirstHttpServletprog</servlet-name>
		<url-pattern>/http/swetha</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FirstGenericServlet</servlet-name>
		<servlet-class>FirstGenericServlet</servlet-class>
	</servlet> 
	<servlet-mapping>
		<servlet-name>FirstGenericServlet</servlet-name>
		<url-pattern>First.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>NameDetails</servlet-name>
		<servlet-class>NameDetailsForm</servlet-class>
	</servlet> 
	<servlet-mapping>
		<servlet-name>NameDetails</servlet-name>
		<url-pattern>/welcome</url-pattern>
	</servlet-mapping>
	
 
    
</web-app>